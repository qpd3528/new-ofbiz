<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="https://ofbiz.apache.org/dtds/entitymodel.xsd">
    <!-- ========================================================= -->
    <!-- ======================== Defaults ======================= -->
    <!-- ========================================================= -->
    <title>Entity of an Apache OFBiz Component</title>
    <description>None</description>
    <version>1.0</version>
    <default-resource-name>BusinessEntityLabels</default-resource-name>

    <!-- ========================================================= -->
    <!-- ======================== Data Model ===================== -->
    <!-- The modules in this file are as follows:                  -->
    <!--  - org.apache.ofbiz.business.business -->
    <!--  - org.apache.ofbiz.business.feature -->
    <!-- ========================================================= -->

    <!-- ========================================================= -->
    <!-- org.apache.ofbiz.business.business -->
    <!-- ========================================================= -->

    <entity entity-name="Business" package-name="org.apache.ofbiz.business.business" title="Business Entity">
        <field name="businessId" type="id"><description>primary sequenced ID</description></field>
        <field name="businessTypeId" type="id"></field>
        <field name="statusId" type="id"></field>
        <field name="businessName" type="name"></field><!--기업명-->
        <field name="description" type="description"></field>
        <field name="longDescription" type="very-long"></field>
        <field name="comments" type="comment"></field>
        <field name="businessSize" type="numeric"></field>
        <field name="businessDate" type="date-time"></field>
        <field name="anotherDate" type="date-time"></field>
        <field name="anotherText" type="long-varchar"></field>

<!--        새로 추가되는 컬럼 리스트-->
        <field name="businessRegistrationCertificateCompanyName" type="name"></field><!--사업자등록증 기업명-->
        <field name="representativeName" type="name"></field><!--대표자-->
        <field name="businessRegistrationNumber" type="id"></field><!--사업자등록번호-->
        <field name="dateOfBusinessCommencement" type="date"></field><!--개업연월일-->
        <field name="address" type="name"></field><!--상세주소-->
        <field name="phoneNum" type="name"></field><!--대표전화번호-->
        <field name="businessType" type="name"></field><!--업태-->
        <field name="businessItem" type="name"></field><!--종목-->
        <field name="emailAddress" type="name"></field><!--이메일-->
        <field name="openingTime" type="time"></field><!--영업 시작 시간-->
        <field name="closingTime" type="time"></field><!--영업 종료 시간-->
        <field name="affiliationTypeId" type="name"></field><!--이메일-->
        <field name="businessClassificationTypeId" type="name"></field><!--영업 시작 시간-->
        <field name="taxTypeId" type="name"></field><!--영업 종료 시간-->
        <field name="majorId" type="id"></field><!-- beacon major id-->
        <prim-key field="businessId"/>

<!--        이렇게 해두면 생성 시에 이 부분을 필수사항으로 받아야 함. 받지 않는 순간 오류 발생-->
        <relation type="one" fk-name="EXMPL_TYP" rel-entity-name="BusinessType">
            <key-map field-name="businessTypeId"/>
        </relation>
        <relation type="one" fk-name="EXMPL_STTS" rel-entity-name="StatusItem">
            <key-map field-name="statusId"/>
        </relation>
        <relation type="one" fk-name="EXMPL_TY" rel-entity-name="AffiliationType">
            <key-map field-name="affiliationTypeId"/>
        </relation>
        <relation type="one" fk-name="EXMPL_STS" rel-entity-name="BusinessClassificationType">
            <key-map field-name="businessClassificationTypeId"/>
        </relation>
        <relation type="one" fk-name="EXMPL_STT" rel-entity-name="TaxType">
            <key-map field-name="taxTypeId"/>
        </relation>
<!--        <relation type="one" fk-name="EXMPL_beacon" rel-entity-name="Beacon">-->
<!--            <key-map field-name="majorId"/>-->
<!--        </relation>-->
<!--        <relation type="one" fk-name="EXMPL_beacon2" rel-entity-name="BusinessBeacon">-->
<!--            <key-map field-name="major2Id"/>-->
<!--        </relation>-->


    </entity>

    <entity entity-name="BusinessItem" package-name="org.apache.ofbiz.business.business" title="Business Item Entity">
        <field name="businessId" type="id"></field>
        <field name="businessItemSeqId" type="id"><description>secondary sequenced ID</description></field>
        <field name="uuid" type="name"></field>
        <field name="majorId" type="id"></field>
        <field name="minorId" type="id"></field>
        <field name="macAddress" type="id"></field>
        <field name="transmissionPower" type="integer"></field>
        <field name="broadcastingInterval" type="integer"></field>
        <field name="description" type="description"></field>
        <field name="amount" type="floating-point"></field>
        <field name="amountUomId" type="id"></field>
        <prim-key field="businessId"/>
        <prim-key field="businessItemSeqId"/>
        <relation type="one" fk-name="EXMPLIT_UOM" title="Amount" rel-entity-name="Uom">
            <key-map field-name="amountUomId" rel-field-name="uomId"/>
        </relation>
        <relation type="one" fk-name="EXMPLIT_EXMP" rel-entity-name="Business">
            <key-map field-name="businessId"/>
        </relation>
    </entity>

<!--    <entity entity-name="BusinessBeacon" package-name="org.apache.ofbiz.business.business" title="Business Beacon Entity">-->
<!--        <field name="businessId" type="id"></field>-->
<!--        <field name="businessBeaconSeqId" type="id"><description>secondary sequenced ID</description></field>-->
<!--        <field name="major2Id" type="id"></field>-->
<!--        <prim-key field="businessId"/>-->
<!--        <prim-key field="businessItemSeqId"/>-->
<!--        <relation type="one" fk-name="EXMPLIT_Beacon" title="Beacon" rel-entity-name="Beacon">-->
<!--            <key-map field-name="major2Id" rel-field-name="beaconId"/>-->
<!--        </relation>-->
<!--        <relation type="one" fk-name="EXMPLIT_EXMP" rel-entity-name="Business">-->
<!--            <key-map field-name="businessId"/>-->
<!--        </relation>-->
<!--    </entity>-->


    <entity entity-name="BusinessStatus" package-name="org.apache.ofbiz.business.business" title="Business Status Entity">
        <field name="businessId" type="id"></field>
        <field name="statusDate" type="date-time"></field>
        <field name="statusEndDate" type="date-time"></field>
        <field name="changeByUserLoginId" type="id-vlong"></field>
        <field name="statusId" type="id"></field>
<!--        <prim-key field="businessId"/>-->
        <prim-key field="statusDate"/>
        <relation type="one" fk-name="EXMPLST_EXMPL" rel-entity-name="Business">
            <key-map field-name="businessId"/>
        </relation>
        <relation type="one" fk-name="EXMPLST_STTS" rel-entity-name="StatusItem">
            <key-map field-name="statusId"/>
        </relation>
        <relation type="one" fk-name="EXMPLST_STTS_USER" rel-entity-name="UserLogin">
            <key-map field-name="changeByUserLoginId" rel-field-name="userLoginId"/>
        </relation>
    </entity>
    <entity entity-name="BusinessType"
            package-name="org.apache.ofbiz.business.business"
            title="Business Type Entity">
        <field name="businessTypeId" type="id"></field>
        <field name="parentTypeId" type="id"></field>
        <field name="description" type="description"></field>
        <prim-key field="businessTypeId"/>
        <relation type="one" fk-name="EXMPLTP_PAR" rel-entity-name="BusinessType">
            <key-map field-name="parentTypeId" rel-field-name="businessTypeId"/>
        </relation>
    </entity>

    <entity entity-name="AffiliationType"
            package-name="org.apache.ofbiz.business.business"
            title="Affiliation Type Entity">
        <field name="affiliationTypeId" type="id"></field>
        <field name="parentTypeId" type="id"></field>
        <field name="description" type="description"></field>
        <prim-key field="affiliationTypeId"/>
        <relation type="one" fk-name="EXMPLTP_PAR" rel-entity-name="AffiliationType">
            <key-map field-name="parentTypeId" rel-field-name="affiliationTypeId"/>
        </relation>
    </entity>

    <entity entity-name="BusinessClassificationType"
            package-name="org.apache.ofbiz.business.business"
            title="BusinessClassification Type Entity">
        <field name="businessClassificationTypeId" type="name"></field>
        <field name="parentTypeId" type="name"></field>
        <field name="description" type="description"></field>
        <prim-key field="businessClassificationTypeId"/>
        <relation type="one" fk-name="EXMPLTP_PAR" rel-entity-name="BusinessClassificationType">
            <key-map field-name="parentTypeId" rel-field-name="businessClassificationTypeId"/>
        </relation>
    </entity>

    <entity entity-name="TaxType"
            package-name="org.apache.ofbiz.business.business"
            title="Tax Type Entity">
        <field name="taxTypeId" type="name"></field>
        <field name="parentTypeId" type="name"></field>
        <field name="description" type="description"></field>
        <prim-key field="taxTypeId"/>
        <relation type="one" fk-name="EXMPLTP_PAR" rel-entity-name="TaxType">
            <key-map field-name="parentTypeId" rel-field-name="taxTypeId"/>
        </relation>
    </entity>



    <!-- ========================================================= -->
    <!-- org.apache.ofbiz.business.feature -->
    <!-- ========================================================= -->

    <entity entity-name="BusinessFeature"
        package-name="org.apache.ofbiz.business.feature"
        title="Business Feature Entity">
        <field name="businessFeatureId" type="id"><description>primary sequenced ID</description></field>
        <field name="featureSourceEnumId" type="id"></field>
        <field name="description" type="description"></field>
        <prim-key field="businessFeatureId"/>
        <relation type="one" fk-name="EXFT_ENUM" rel-entity-name="Enumeration">
            <key-map field-name="featureSourceEnumId" rel-field-name="enumId"/>
        </relation>
    </entity>
    <entity entity-name="BusinessFeatureAppl"
        package-name="org.apache.ofbiz.business.feature"
        title="Business Feature Application Entity">
        <field name="businessId" type="id"></field>
        <field name="businessFeatureId" type="id"></field>
        <field name="fromDate" type="date-time"></field>
        <field name="thruDate" type="date-time"></field>
        <field name="businessFeatureApplTypeId" type="id"></field>
        <field name="sequenceNum" type="numeric"></field>
        <prim-key field="businessId"/>
        <prim-key field="businessFeatureId"/>
        <prim-key field="fromDate"/>
        <relation type="one" fk-name="EXFTAP_EXPL" rel-entity-name="Business">
            <key-map field-name="businessId"/>
        </relation>
        <relation type="one" fk-name="EXFTAP_EXFT" rel-entity-name="BusinessFeature">
            <key-map field-name="businessFeatureId"/>
        </relation>
        <relation type="one" fk-name="EXFTAP_TYP" rel-entity-name="BusinessFeatureApplType">
            <key-map field-name="businessFeatureApplTypeId"/>
        </relation>
    </entity>
    <entity entity-name="BusinessFeatureApplType"
        package-name="org.apache.ofbiz.business.feature"
        title="Business Feature Application Type Entity">
        <field name="businessFeatureApplTypeId" type="id"></field>
        <field name="parentTypeId" type="id"></field>
        <field name="description" type="description"></field>
        <prim-key field="businessFeatureApplTypeId"/>
        <relation type="one" fk-name="EXFTAPTP_PAR" rel-entity-name="BusinessFeatureApplType">
            <key-map field-name="parentTypeId" rel-field-name="businessFeatureApplTypeId"/>
        </relation>
    </entity>
</entitymodel>
